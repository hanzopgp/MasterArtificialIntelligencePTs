(enc)\n","    mu + sig * torch.randn(sig.size()).to(device)  # reparam trick\n","\n","    self.reg_loss = -0.5 * torch.sum(1 + torch.log(sig**2) - mu**2 - sig**2)\n","\n","    cat = torch.cat((mu, sig), dim=1)\n","    reconstruct = self.decoder(cat)\n","    self.rec_loss = torch.nn.CrossEntropyLoss()(reconstruct, x)\n","\n","    return reconstruct, self.rec_loss.cpu().item()\n","\n","  def backward(self):\n","    self.rec_loss.backward(retain_graph=True)\n","    self.reg_loss.backward()\n"],"metadata":{"id":"C51Bf4JXe00X","executionInfo":{"status":"ok","timestamp":1670251526405,"user_tz":-60,"elapsed":3,"user":{"displayName":"Damien Dam","userId":"03495965520933490918"}}},"execution_count":7,"outputs":[]},{"cell_type":"code","source":["N_EPOCHS = 20\n","model = VAE()\n","optim = torch.optim.Adam(model.parameters())\n","\n","for i in range(N_EPOCHS):\n","  print(\"\\nEpoch\", i)\n","\n","  model.train()\n","  loss = 0.\n","  nb = 0\n","  for x, _ in train_loader:\n","    optim.zero_grad()\n","    nb += len(x)\n","    x = torch.flatten(x, start_dim=1)\n","    loss += model(x)[1]\n","    model.backward()\n","    optim.step()\n","  print(\"Train loss:\", loss / nb)\n","  \n","  model.eval()\n","  loss = 0.\n","  nb = 0\n","  for x, _ in test_loader:\n","    nb += len(x)\n","    x = torch.flatten(x, start_dim=1)\n","    loss += model(x)[1]\n","  print(\"Test loss:\", loss / nb)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":485},"id":"pos7hVbXe3R1","executionInfo":{"status":"error","timestamp":1670251528570,"user_tz":-60,"elapsed":7,"user":{"displayName":"Damien Dam","userId":"03495965520933490918"}},"outputId":"a13fbb05-6bc0-4edf-e31a-0782fc6ffac2"},"execution_count":8,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Epoch 0\n"]},{"output_type":"error","ename":"RuntimeError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mRuntimeError\u00